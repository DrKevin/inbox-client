{
  "name": "inbox-client",
  "productName": "Inbox",
  "version": "1.0.0",
  "description": "A desktop client for Google Inbox that supports multiple accounts.",
  "main": "./src/index.js",
  "scripts": {
    "start": "electron ./src/index.js",
    "build": "electron-packager . $npm_package_productName --out=dist --platform=darwin --arch=x64 icon=Icon.icns --ignore='^/dist$' --version=$npm_package_electronVersion --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atom/electron-quick-start.git"
  },
  "keywords": [
    "electron",
    "inbox",
    "gmail"
  ],
  "author": "Dominic McPhee",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/atom/electron-quick-start/issues"
  },
  "homepage": "https://github.com/atom/electron-quick-start#readme",
  "engines": {
    "node": ">=4"
  },
  "electronVersion": "0.35.0",
  "devDependencies": {
    "electron-prebuilt": "^0.35.0",
    "electron-packager": "^5.1.1"
  },
  "dependencies": {
    "babel": "^6.3.13",
    "babel-core": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "chance": "^0.8.0",
    "nedb": "^1.5.0"
  }
}
